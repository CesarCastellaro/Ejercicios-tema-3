public class Main {
    public static void main(String[] args) {
            coche micoche = new coche();
            micoche.agregarPuerta();
            micoche.agregarPuerta();
            micoche.agregarPuerta();

            System.out.println(micoche.puertas);
    }
}
class coche{
    public int puertas = 0;
    public void agregarPuerta () {
        this.puertas++;
    }
}
